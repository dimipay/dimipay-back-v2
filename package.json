{
  "name": "express-restapi-boilerplate",
  "version": "1.0.0",
  "description": "",
  "main": "src/index.js",
  "scripts": {
    "ts-run": "ts-node --transpileOnly -r tsconfig-paths/register --files",
    "start": "cross-env NODE_ENV=prod yarn ts-run ./src/index.ts",
    "dev": "yarn db:pull && cross-env NODE_ENV=develop nodemon --watch src/**/*.ts --exec yarn ts-run src/index.ts",
    "dev:dry": "cross-env NODE_ENV=develop nodemon --watch 'src/**/*.ts' --exec yarn ts-run src/index.ts",
    "db": "npx prisma db push && npx prisma db pull && npx prisma generate",
    "db:pull": "prisma db pull && prisma generate",
    "docs": "yarn ts-run docs/cli.ts"
  },
  "author": "",
  "license": "ISC",
